var n0 = 0
var n1 = 1
var temp = 0
var line = ""

when game starts
	while n0 < 100.0
		line = line + n0 + ", "
		temp = n1
		n1 = n1 + n0
		n0 = temp
	print(line)
		
sprite sprite1
	when game starts
		while n0 < 100.0
			print("I am an eventhandler!")
			
	when game starts
		while n0 < 100.0
			print("I am another eventhandler!")	
			
sprite sprite2
	when game starts
		while n0 < 100.0
			print("I am yet another sprite!")